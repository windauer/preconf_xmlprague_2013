<?xml version="1.0" encoding="UTF-8"?>
<xf:model xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xf="http://www.w3.org/2002/xforms">
    <xf:instance id="i-default">
        <data xmlns="">
            <query/>
            <startyear>1869</startyear>
            <endyear>2012</endyear>
            <order>year</order>
            <start>1</start>
            <max>10</max>
            <code/>
            <startdate/>
            <enddate/>
            <sortAscending>true</sortAscending>
        </data>
    </xf:instance>
    <xf:bind nodeset="instance('i-default')">
        <xf:bind nodeset="startdate"/>
        <xf:bind nodeset="enddate"/>
        <xf:bind nodeset="sortAscending" type="boolean"/>
    </xf:bind>
    <xf:instance id="i-codes" resource="/exist/apps/gesetze/data/transformed/codes-toc.xml"/>
    <xf:instance id="i-years" resource="/exist/apps/gesetze/data/transformed/codes-by-year.xml"/>
    <xf:submission id="s-submit" method="get" ref="instance()" replace="embedXFormsUI" targetid="searchResultMount" resource="modules/search.xql" validate="false">
        <xf:message ev:event="xforms-submit-error">Submission 's-submit' failed</xf:message>
        <xf:message ev:event="xforms-submit-done" level="ephemeral">finished searching</xf:message>
    </xf:submission>
    <xf:submission id="s-obay" method="get" ref="instance()" replace="embedXFormsUI" targetid="searchResultMount" resource="searchResult.html" validate="false">
        <xf:message ev:event="xforms-submit-error">Submission 's-obay' failed</xf:message>
        <xf:message ev:event="xforms-submit-done" level="ephemeral">finished searching</xf:message>
    </xf:submission>
    <xf:submission id="s-call-xquery" method="get" ref="instance('i-default')" replace="embedHTML" targetid="searchResultMount" resource="modules/search.xql" validate="false">
        <xf:action ev:event="xforms-submit-error">
            <xf:message>Submission 's-call-xquery' failed</xf:message>
        </xf:action>
    </xf:submission>
    <xf:submission id="s-call-html" method="get" replace="embed" targetid="searchResultMount" resource="xquick.html?query={instance()/xquery}" validate="false">
        <xf:action ev:event="xforms-submit-error">
            <xf:message>Submission 'submit' failed</xf:message>
        </xf:action>
    </xf:submission>
</xf:model>