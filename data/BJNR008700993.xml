    <norm builddate="20120625152716" doknr="BJNR008700993BJNE001000333">
        <metadaten>
            <jurabk>BetTerMstrV</jurabk>
            <enbez>§ 6</enbez>
            <titel format="parat">Übergangsvorschrift</titel>
        </metadaten>
        <textdaten>
