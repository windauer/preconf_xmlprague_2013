            <titleStmt>
                <title>Verordnung über die Festsetzung des Lärmschutzbereichs für den militärischen Flugplatz Fürstenfeldbruck</title>
                <title type="short">FluLärmFürstenfV</title>
            </titleStmt>
            <publicationStmt>
                <date>1979-07-12</date>
            </publicationStmt>
