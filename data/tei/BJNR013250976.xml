            <titleStmt>
                <title>Verordnung über die Festsetzung des Lärmschutzbereichs für den militärischen Flugplatz Hopsten</title>
                <title type="short">FluLärmHopstV</title>
            </titleStmt>
            <publicationStmt>
                <date>1976-05-26</date>
            </publicationStmt>
        </fileDesc>
