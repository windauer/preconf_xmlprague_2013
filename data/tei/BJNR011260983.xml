        <fileDesc>
            <titleStmt>
                <title>Bekanntmachung zu § 4 des Warenzeichengesetzes</title>
                <title type="short">WZG§4AIPOBek</title>
            </titleStmt>
            <publicationStmt>
                <date>1983-08-09</date>
            </publicationStmt>
