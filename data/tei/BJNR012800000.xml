            <titleStmt>
                <title>Verordnung über die Festsetzung des Lärmschutzbereichs für den militärischen Flugplatz Spangdahlem</title>
                <title type="short">FluLärmSpangV</title>
            </titleStmt>
            <publicationStmt>
                <date>2000-02-16</date>
            </publicationStmt>
